@model IEnumerable<Adilet.Models.Makale>
<h5 class="card-header">En Son Eklenenler</h5>
<div class="card-body">
    <div class="row">
        <div class="col-lg-12">
            <ul class="list-unstyled mb-0">
                @foreach (var makale in Model)
                {
                    <li>
                        <a href="~/Home/MakaleDetay/@makale.MakaleId"> @makale.Baslik
                        (@makale.Kategori.KategoriAdi)
                    </a>
                </li>
                }
            </ul>
        </div>
    </div>
</div>

